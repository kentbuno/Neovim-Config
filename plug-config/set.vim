filetype plugin indent on
set gcr=
set tgc
syntax enable
set number
set nowrap
set ts=4
set autoindent
set expandtab
set shiftwidth=4
set cursorline
set showmatch
set scrolloff=5 
set relativenumber
set noshowmode
set showtabline=2
set nobackup
set nowritebackup  
set encoding=utf-8                        
set noswapfile
set t_Co=256
set smarttab
set ttyfast
set backspace=indent,eol,start
set laststatus=2
set incsearch	            
set nohlsearch
set signcolumn=yes




